<section local-class="section">
  {{link-to 'All Canvases' 'team' (query-params channel=null) local-class='item item--large item--selected'}}
</section>

<section local-class="section">
  <h5 local-class="heading">Channels</h5>

  {{#each channels as |channel|}}
    {{#link-to 'team' (query-params channel=channel.name) local-class='item'}}
      <span local-class="channel-prefix">#</span> {{channel.name}}
    {{/link-to}}
  {{/each}}
</section>
