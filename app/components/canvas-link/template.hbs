{{#yielding-link-to 'team.canvas.show' canvasID
  (if (and blockID filter)
    (query-params block=blockID filter=filter)
    (if blockID
      (query-params block=blockID)
      (if filter
        (query-params filter=filter)
        (query-params))))
  class=class as |link|}}
  {{#if hasBlock}}
    {{yield}}
  {{else}}
    {{protocol}}//{{host}}{{link.href}}
  {{/if}}
{{/yielding-link-to}}
