{{#if personalTeam.domain}}
  {{#link-to "team.index" personalTeam
    local-class='item'
    activeClass=styles.item--active}}
    {{team-list-item team=personalTeam}}
  {{/link-to}}
{{else}}
  {{#link-to "setup"
    local-class='item'
    activeClass=styles.item--active}}
    {{team-list-item team=personalTeam}}
  {{/link-to}}
{{/if}}

{{#each slackTeams as |team|}}
  {{#link-to "team.index" team
    local-class='item'
    activeClass=styles.item--active}}
    {{team-list-item team=team}}
  {{/link-to}}
{{/each}}

{{#link-to 'login' local-class='item item--new'}}
  {{svg-jar "Add" local-class='new-icon' height='32' width='32'}}
{{/link-to}}
